<?xml version="1.0" encoding="UTF-8"?>
<!--
    This XML file allows us to easily customize the look and language used
    in our program's user interface. Note that this properties file is only
    for the workspace controls, which are custom for each application
-->
<properties>
    <property_list>
        <!-- PROGRAM ICON FILES -->
    <property name="PHOTO_ICON"		    value="Photo.png"></property>
	<property name="CODE_ICON"		    value="Code.png"></property>
	<property name="SELECT_ICON"	    value="Select.png"></property>
	<property name="RESIZE_ICON"	    value="Resize.png"></property>
	<property name="ADD_CLASS_ICON"		value="AddClass.png"></property>
	<property name="ADD_INTERFACE_ICON"		value="AddInterface.png"></property>
	<property name="REMOVE_ICON"		value="Remove.png"></property>
	<property name="UNDO_ICON"	        value="Undo.png"></property>
	<property name="REDO_ICON"		    value="Redo.png"></property>
	<property name="ZOOM_IN_ICON"		value="ZoomIn.png"></property>
	<property name="ZOOM_OUT_ICON"		value="ZoomOut.png"></property>
	<property name="PLUS_ICON"		value="Plus.png"></property>
	<property name="MINUS_ICON"		value="Minus.png"></property>
        
        <!-- TOOLTIPS FOR BUTTONS -->
	<property name="PHOTO_TOOLTIP"		    value="Save design as image"></property>
	<property name="CODE_TOOLTIP"		    value="Export design to Java Code"></property>
	<property name="SELECT_TOOLTIP"	        value="Select an element"></property>
	<property name="RESIZE_TOOLTIP"	        value="Resize an element"></property>
	<property name="ADD_CLASS_TOOLTIP"		value="Add a Java class"></property>
	<property name="ADD_INTERFACE_TOOLTIP"		value="Add a Java interface"></property>
	<property name="REMOVE_TOOLTIP"		value="Remove an element"></property>
	<property name="UNDO_TOOLTIP"	        value="Undo last action"></property>
	<property name="REDO_TOOLTIP"		    value="Redo action"></property>
	<property name="ZOOM_IN_TOOLTIP"		value="Zoom In"></property>
	<property name="ZOOM_OUT_TOOLTIP"		value="Zoom Out"></property>
	
	<!-- MESSAGES -->
	<property name="REMOVE_ELEMENT_MESSAGE"	    value="Are you sure you wish to remove that element?"></property>
		
	<!-- ERROR MESSAGES AND TITLES FOR THEIR DIALOGS -->
	<property name="ATTRIBUTE_UPDATE_ERROR_MESSAGE"	    value="WARNING: An error occured updating the element's attribute."></property>
	<property name="ATTRIBUTE_UPDATE_ERROR_TITLE"	    value="Attribute Update Error"></property>
	<property name="UPDATE_ERROR_MESSAGE"		    value="An error occured updating the UI"></property>
	<property name="UPDATE_ERROR_TITLE"		    value="UI Update Error"></property>
    </property_list>
    <property_options_list>
    </property_options_list>
</properties>